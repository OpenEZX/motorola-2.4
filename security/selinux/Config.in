bool 'NSA SELinux Support' CONFIG_SECURITY_SELINUX
if [ "$CONFIG_SECURITY_SELINUX" != "n" ]; then
   bool '  NSA SELinux Development Support' CONFIG_SECURITY_SELINUX_DEVELOP 
   if [ "$CONFIG_EXPERIMENTAL" = "y" ]; then
      bool '  NSA SELinux MLS policy (EXPERIMENTAL)' CONFIG_SECURITY_SELINUX_MLS 
      dep_bool '  NSA SELinux extended socket calls (EXPERIMENTAL)' CONFIG_SECURITY_SELINUX_EXTSOCKET $CONFIG_SECURITY_NETWORK
      dep_bool '  Labeled IP Networking Support' CONFIG_SECURITY_SELINUX_NSID $CONFIG_NETFILTER $CONFIG_SECURITY_NETWORK
      dep_tristate '    CIPSO/FIPS-188 IP Options (EXPERIMENTAL)' CONFIG_SECURITY_SELINUX_SELOPT $CONFIG_SECURITY_SELINUX_NSID
   fi
fi


